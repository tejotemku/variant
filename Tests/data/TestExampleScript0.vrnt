string iftest(int res)
{
	if ((res == 0 + 1 - 3%2 )  || ("aaga" == "aaa"))
	#if (-(res == 0))
	{		
		return "True";
	}
	else 
	{
		return "False";
	}
}

int silnia(int l)
{
	if(l == 1)
	{
		return 1;
	}
	else
	{
		return l*silnia(l-1);
	}
}